{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "coffeeshop_app",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "coffeeshop_app (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "coffeeshop_app (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        },
        {
            "name": "coffeeshop_app (edge-browser)",
            "type": "dart",
            "request": "launch",
            "program": "lib/main.dart",
            "args": [
              "-d",
              "edge"
            ]
        },
        {
            "name": "coffeeshop_app (chrome-no-cors)",
            "type": "dart",
            "request": "launch",
            "program": "lib/main.dart",
            "args": [
              "-d",
              "chrome",
              "--web-browser-flag",
              "--disable-web-security"
            ]
        }
    ]
}